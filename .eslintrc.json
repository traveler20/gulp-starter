{
    "env": {
        "browser": true, //ブラウザでメジャーなもの（documentなど）を許可
        "es6": true //ES6の書き方許可
    },
    "extends": [
        "eslint:recommended", //ESLintおすすめルールの読み込み
        "prettier" //Prettier適用
    ],
    //個別ルール
    "rules": {
        "no-extra-semi": "warn", //セミコロンがないと警告
        "quotes": [
            //シングルクオートを使っていないと警告
            "warn"
            // ,"single"
        ]
    }
}
